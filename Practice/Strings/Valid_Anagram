class Solution {
public:
    bool isAnagram(string s, string t) {
        
        if(s.length() != t.length())return false;
        int map[256]={0};

        for(auto ch: s){
            map[ch]++;
        }

        for(int i=0; i<t.length(); i++){
            if(map[t[i]] != 0){
                map[t[i]]--;
            }
            else return false;
        }
        return true;
    }
};