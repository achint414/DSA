int start=0;
int end=size-1;

int mid=start + (end-start)/2; 
                                   
while(start<=end){

    if(arr[mid]==key){
        return mid;
    }
    if(mid-1>=0 && arr[mid-1]==key){
        return mid-1;
    }
    if(mid+1<size && arr[mid+1]==key){
        return mid+1;
    }
    
    else if(target > arr[mid]){
        start=mid+2;
    }
    
    else{
        end=mid-2;
    }

    mid=start + (end-start)/2;
}
