 class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int maxi=INT_MIN;
        int sum=0;
        int length=nums.size();

        for(int i=0;i<length;i++){
            sum+=nums[i];
            maxi=max(maxi,sum);
            if(sum<0){
                sum=0;
            }
        }
        //if (maxi<0) maxi=0;
        return maxi;
    }
};